{
  "name": "acsstatsangular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "npm run update-timestamp && node aspnetcore-https",
    "update-timestamp": "node ./bin/update-timestamp.js",
    "start": "run-script-os",
    "start:windows": "ng serve --port 44410 --ssl --ssl-cert %APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem --ssl-key %APPDATA%\\ASP.NET\\https\\%npm_package_name%.key",
    "start:default": "ng serve --port 44410 --ssl --ssl-cert $HOME/.aspnet/https/${npm_package_name}.pem --ssl-key $HOME/.aspnet/https/${npm_package_name}.key",
    "start:defaultNoSSl": "ng serve --port 44410",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.5",
    "@angular/cdk": "^19.1.5",
    "@angular/common": "^19.2.5",
    "@angular/compiler": "^19.2.5",
    "@angular/core": "^19.2.5",
    "@angular/forms": "^19.2.5",
    "@angular/platform-browser": "^19.2.5",
    "@angular/platform-browser-dynamic": "^19.2.5",
    "@angular/platform-server": "^19.2.5",
    "@angular/router": "^19.2.5",
    "@fortawesome/angular-fontawesome": "^1.0.0",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@ngrx/effects": "^19.0.1",
    "@ngrx/store": "^19.0.1",
    "@ngrx/store-devtools": "^19.0.1",
    "@ngworker/lumberjack": "^19.0.0",
    "@primeng/themes": "^19.0.9",
    "bootstrap": "^5.3.3",
    "dompurify": "^3.2.5",
    "file-saver": "^2.0.5",
    "jquery": "^3.7.1",
    "luxon": "^3.5.0",
    "oidc-client": "^1.11.5",
    "popper.js": "^1.16.1",
    "primeng": "^19.0.9",
    "run-script-os": "^1.1.6",
    "rxjs": "~7.8.1",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.6",
    "@angular/cli": "^19.2.6",
    "@angular/compiler-cli": "^19.2.5",
    "@types/file-saver": "^2.0.7",
    "@types/jasmine": "~5.1.4",
    "@types/jasminewd2": "~2.0.13",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.13.4",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "typescript": "~5.7.3"
  }
}
